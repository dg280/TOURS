{
    "rewrites": [
        {
            "source": "/admin",
            "destination": "/admin.html"
        },
        {
            "source": "/live/(.*)",
            "destination": "/live.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/reminders",
            "schedule": "0 9 * * *"
        }
    ]
}